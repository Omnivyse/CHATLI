# CHATLI Mobile App Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
API_BASE_URL=chatli-production.up.railway.app/api
SOCKET_URL=chatli-production.up.railway.app

# Development URLs (for local testing)
DEV_API_URL=http://localhost:5000/api
DEV_SOCKET_URL=http://localhost:5000

# App Configuration
APP_NAME=CHATLI
APP_VERSION=1.0.0

# Optional: Analytics (if you add analytics later)
ANALYTICS_API_KEY=your_analytics_key_here

# Optional: Push Notifications (if you add push notifications later)
EXPO_PUSH_TOKEN=your_expo_push_token_here

# Optional: Error Reporting (like Sentry)
SENTRY_DSN=your_sentry_dsn_here

# Development Settings
DEBUG_MODE=false
LOG_LEVEL=info 